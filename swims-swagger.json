{
  "swagger": "2.0",
  "info": {
    "title": "Third Party API",
    "version": "v1"
  },
  "paths": {
    "/swims/getVendorClubs/vendor/{vendorId}": {
      "get": {
        "tags": ["SwimsThirdParty"],
        "produces": ["text/plain", "application/json", "text/json"],
        "description": "Get all the clubs that a vendor has access to.",
        "parameters": [
          {
            "in": "path",
            "name": "vendorId",
            "required": true,
            "type": "integer",
            "format": "int32"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SwimsThirdPartyVendorClubs"
              }
            }
          }
        }
      }
    },
    "/swims/getMemberDetails/club/{clubId}": {
      "get": {
        "tags": ["SwimsThirdParty"],
        "produces": ["text/plain", "application/json", "text/json"],
        "description": "Get the details of a member or a list of members for a club. \nIf the MemberId is not provided, the list of members for the club is returned, otherwise the details of the member are returned. \nThe Registration Array returns the most recent registration of a member [athlete, coach, official, misc]",
        "parameters": [
          {
            "in": "path",
            "name": "clubId",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "memberId",
            "type": "string"
          },
          {
            "in": "query",
            "name": "lastModified",
            "type": "string",
            "format": "date-time"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SwimsMemberDetail"
              }
            }
          }
        }
      }
    },
    "/swims/getRegistrationLink/club/{clubId}": {
      "get": {
        "tags": ["SwimsThirdParty"],
        "produces": ["text/plain", "application/json", "text/json"],
        "parameters": [
          {
            "in": "path",
            "name": "clubId",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "$ref": "#/definitions/SwimsRegistrationLink"
            }
          }
        }
      }
    },
    "/swims/getAllEventsWithinDateTime/vendor/{vendorId}/from/{fromDateTime}": {
      "get": {
        "tags": ["SwimsThirdParty"],
        "produces": ["text/plain", "application/json", "text/json"],
        "parameters": [
          {
            "in": "path",
            "name": "vendorId",
            "required": true,
            "type": "integer",
            "format": "int32",
            "description": "Alphanumeric ID that is a vendorâ€™s unique identifier"
          },
          {
            "in": "path",
            "name": "fromDateTime",
            "required": true,
            "type": "string",
            "format": "date-time"
          },
          {
            "in": "query",
            "name": "toDateTime",
            "type": "string",
            "format": "date-time"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SwimsEvent"
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "SwimsEvent": {
      "type": "object",
      "properties": {
        "eventSequence": {
          "format": "int32",
          "type": "integer"
        },
        "eventType": {
          "type": "string"
        },
        "clubId": {
          "type": "string"
        },
        "eventData": {
          "$ref": "#/definitions/SwimsEventData"
        }
      }
    },
    "SwimsEventData": {
      "type": "object",
      "properties": {
        "vendorRecordId": {
          "type": "string"
        },
        "eventDataContextName": {
          "type": "string"
        },
        "contextIds": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "SwimsMemberContactInformation": {
      "type": "object",
      "properties": {
        "contactFirstName": {
          "type": "string"
        },
        "contactLastName": {
          "type": "string"
        },
        "contactEmail": {
          "type": "string"
        },
        "contactPhone": {
          "type": "string"
        }
      }
    },
    "SwimsMemberDetail": {
      "type": "object",
      "description": "Member details",
      "properties": {
        "memberId": {
          "type": "string"
        },
        "oldUsasId": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "preferredName": {
          "type": "string"
        },
        "middleName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "birthDate": {
          "type": "string"
        },
        "competitionCategory": {
          "type": "string"
        },
        "contactInformation": {
          "$ref": "#/definitions/SwimsMemberContactInformation"
        },
        "registrations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SwimsMemberRegistration"
          },
          "description": "List of most recent registrations of each type for a member",
          "example": [
            {
              "registrationDate": "1652122270",
              "expirationDate": "1715280670",
              "registrationId": 49492932,
              "registrationType": "coach",
              "transactionType": "string",
              "lscCode": "",
              "clubCode": "string",
              "usasFee": 400,
              "lscFee": 200,
              "isRenewal": true,
              "isAttached": true,
              "memberGoodStandingExpirationDate": "1715280670"
            },
            {
              "registrationDate": "1652122270",
              "expirationDate": "1715280670",
              "registrationId": 49492932,
              "registrationType": "official",
              "transactionType": "string",
              "lscCode": "",
              "clubCode": "string",
              "usasFee": 400,
              "lscFee": 200,
              "isRenewal": true,
              "isAttached": true,
              "memberGoodStandingExpirationDate": "1715280670"
            },
            {
              "registrationDate": "1652122270",
              "expirationDate": "1715280670",
              "registrationId": 49492932,
              "registrationType": "athlete",
              "transactionType": "string",
              "lscCode": "",
              "clubCode": "string",
              "usasFee": 400,
              "lscFee": 200,
              "isRenewal": true,
              "isAttached": true,
              "memberGoodStandingExpirationDate": "1715280670"
            }
          ]
        }
      }
    },
    "SwimsMemberRegistration": {
      "type": "object",
      "properties": {
        "registrationDate": {
          "type": "string"
        },
        "expirationDate": {
          "type": "string"
        },
        "registrationId": {
          "format": "int32",
          "type": "integer"
        },
        "registrationType": {
          "type": "string",
          "example": "coach"
        },
        "transactionType": {
          "type": "string"
        },
        "lscCode": {
          "type": "string"
        },
        "clubCode": {
          "type": "string"
        },
        "usasFee": {
          "format": "double",
          "type": "number"
        },
        "lscFee": {
          "format": "double",
          "type": "number"
        },
        "isRenewal": {
          "type": "boolean"
        },
        "isAttached": {
          "type": "boolean"
        },
        "memberGoodStandingExpirationDate": {
          "type": "string"
        }
      }
    },
    "SwimsThirdPartyVendorClubs": {
      "type": "object",
      "properties": {
        "lscCode": {
          "type": "string"
        },
        "clubCode": {
          "type": "string"
        },
        "clubId": {
          "type": "string"
        }
      }
    },
    "SwimsRegistrationLink": {
      "type": "object",
      "properties": {
        "vendorKey": {
          "type": "string",
          "example": "50i459494294842938nfudfh"
        },
        "registrationUrl": {
          "type": "string",
          "example": "https://www.usaswimming.org/registration/49492932?vendorKey=50i459494294842938nfudfh"
        }
      }
    }
  }
}
